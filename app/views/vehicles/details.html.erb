<%= render partial: "shared/employeeheader" %>
<%= form_for @vehicle_spec,  url: "/employee/welcome", html: {class: "mx-auto col-10 col-md-8 col-lg-6"} do |f| %>
<div class="form-group">
  <%= f.label :user_Id %>
  <%= f.text_field :user_id, class: "form-control", readonly: true%>
  </div>
  <%= f.label :Brand%>
  <%= f.text_field :make, class: "form-control", readonly: true %>
  </div>
  <div class="form-group">
  <%= f.label :Model%>
  <%= f.text_field :vehicle_modal, class: "form-control",  readonly: true%>
  </div>
<div class="form-group">
  <%= f.label :color%>
  <%= f.text_field :color, class: "form-control" , readonly: true %>
  </div>
<div class="form-group">
  <%= f.label :vehicle_number%>
  <%= f.text_field :vehicle_number, class: "form-control" , readonly: true %>
  </div>
<div class="form-group">
  <%= f.label :purchase_date%>
  <%= f.text_field :purchase_date, class: "form-control", readonly: true %>
  </div>
<% end %>