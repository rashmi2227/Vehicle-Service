<%= render partial: "shared/employeeheader" %>
<%= form_for @service, url: "/update/status/#{params[:id]}", method: :patch, html: {class: "mx-auto col-10 col-md-8 col-lg-6"} do |f| %>
<div class="form-group">
  <%= f.label :service_Id%>
  <%= f.text_field :id, class: "form-control", readonly: true %>
  </div>
  <div class="form-group">
  <%= f.label :user_Id %>
  <%= f.text_field :user_id, class: "form-control",  readonly: true%>
  </div>
<div class="form-group">
  <%= f.label :vehicle_Id %>
  <%= f.text_field :vehicle_id, class: "form-control" , readonly: true %>
  </div>
<div class="form-group">
  <%= f.label :start_Date %>
  <%= f.text_field :start_date, class: "form-control" , readonly: true %>
  </div>
<div class="form-group">
  <%= f.label :end_Date %>
  <%= f.text_field :end_date, class: "form-control", readonly: true %>
  </div>
  <div class="form-group">
  <%= f.label :status %>
  <%= f.text_field :status, class: "form-control"%>
  </div>
<div class="form-group">
  <%= f.submit "Update Vehicle", class: "btn btn-primary"  %>
  </div>
<% end %>